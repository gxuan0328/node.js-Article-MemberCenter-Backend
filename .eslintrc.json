{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": "warn",
        "quotes": [
            1,
            "single"
        ],
        "no-unused-vars": "warn",
        "no-undef": "off"
    }
}
